{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["custom.d.ts","env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "lib": ["ES2021","dom"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
